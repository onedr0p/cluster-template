# Generic
PUID={{ puid }}
PGID={{ pgid }}
TZ={{ tz }}

# Docker
DOCKERDIR={{ docker_dir }}

# Domains
DOMAINNAME={{ generic__domain_name }}
ORGANIZATION_NAME="{{ generic__organization_name }}"

# Apps
## Cloudflare
CLOUDFLARE_EMAIL={{ apps_cloudflare_email }}
CLOUDFLARE_API_TOKEN={{ apps__cloudflare_api_token }}

CLOUDFLARE_DOMAIN_1_FQDN={{ apps__cloudflare_domain_1_fqdn }}
CLOUDFLARE_DOMAIN_1_ZONE_ID={{ apps__cloudflare_domain_1_zone_id }}

CLOUDFLARE_DOMAIN_1_OBFUSCATED_FQDN={{ apps__cloudflare_domain_1_obfuscated_fqdn }}

## OAuth2 Proxy
OAUTH2_PROXY_WHITELIST_DOMAINS: {{ apps__oauth2_proxy_whitelist_domains }}
OAUTH2_PROXY_CLIENT_ID={{ apps__oauth2_proxy_client_id }}
OAUTH2_PROXY_CLIENT_SECRET={{ apps__oauth2_proxy_client_secret }}
OAUTH2_PROXY_COOKIE_SECRET={{ apps__oauth2_proxy_cookie_secret }}
OAUTH2_PROXY_COOKIE_NAME={{ apps__oauth2_proxy_cookie_name }}
OAUTH2_PROXY_REDIRECT_URL={{ apps__oauth2_proxy_redirect_url }}
OAUTH2_PROXY_AZURE_TENANT_ID={{ apps__oauth2_proxy_azure_tenant_id }}
OAUTH2_PROXY_AZURE_CLIENT_ID={{ apps__oauth2_proxy_azure_client_id }}
OAUTH2_PROXY_AZURE_CLIENT_SECRET={{ apps__oauth2_proxy_azure_client_secret }}
OAUTH2_PROXY_AZURE_EMAIL_DOMAIN={{ apps__oauth2_proxy_azure_email_domain }}

## Unifi
UNIFI_MONGO_PASS={{ apps__unifi_db_password }}
